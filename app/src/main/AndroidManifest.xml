<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.NCEPU">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.FLASHLIGHT" />
    <uses-permission android:name="android.permission.READ_CALENDAR" />
    <uses-permission android:name="android.permission.WRITE_CALENDAR" /> <!-- 获取GSM（2g）、WCDMA（联通3g）等网络状态的信息 -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" /> <!-- 获取wifi网络状态的信息 -->
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" /> <!-- 保持CPU 运转，屏幕和键盘灯有可能是关闭的,用于文件上传和下载 -->
    <uses-permission android:name="android.permission.WAKE_LOCK" /> <!-- 允许读取手机状态 用于创建BmobInstallation -->
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" /> <!-- 用于进行网络定位 -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" /> <!-- 用于访问GPS定位 -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" /> <!-- 用于获取wifi的获取权限，wifi信息会用来进行网络定位 -->
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" /> <!-- 用于访问网络，网络定位需要上网 -->
    <uses-permission android:name="android.permission.INTERNET" /> <!-- 用于读取手机当前的状态 -->
    <uses-permission android:name="android.permission.READ_PHONE_STATE" /> <!-- 用于申请调用A-GPS模块 -->
    <uses-permission android:name="android.permission.ACCESS_LOCATION_EXTRA_COMMANDS" />

    <uses-feature android:name="android.hardware.camera" />

    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FIFESYSTEMS" />
    <uses-permission
        android:name="android.permission.MOUNT_FORMAT_FILESYSTEMS"
        tools:ignore="ProtectedPermissions" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_edm"
        android:label="NCEPU-EDM"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".Student.User.GitHubActivity"></activity>
        <activity android:name=".Student.Query.DailyActivity" />
        <activity android:name=".Student.Predict.MajorActivity" />
        <activity android:name=".Student.Predict.PoliticsActivity" />
        <activity android:name=".Student.Predict.KNNActivity" />
        <activity android:name=".Student.Predict.SVMActivity" />
        <activity android:name=".Student.Predict.RFActivity" />
        <activity android:name=".Student.Predict.NaiveBayesActivity" />
        <activity android:name=".Student.Predict.DecisionTreeActivity" />
        <activity android:name=".Student.Predict.AdaBoostActivity" />
        <activity android:name=".Student.Query.AbilityActivity" />
        <activity android:name=".Student.Predict.ShuLiActivity" />
        <activity android:name=".Student.Query.ScorePdfActivity" />
        <activity android:name=".Student.Query.GradeFailedActivity" />
        <activity android:name=".Student.Query.MajorGPASortActivity" />
        <activity android:name=".Student.Query.SingleSubjectMajorActivity" />
        <activity android:name=".Student.Query.GradeGpaActivity" />
        <activity android:name=".Student.User.CSDNActivity" />
        <activity android:name=".Student.Query.ZCActivity" />
        <activity android:name=".Student.Query.TeachingEvaluationActivity" />
        <activity android:name=".Student.Query.TeachingPlanActivity" />
        <activity android:name=".Student.Query.TendencyActivity" />
        <activity android:name=".Student.Query.GPASortActivity" />
        <activity android:name=".Student.Query.GPAActivity" />
        <activity android:name=".Student.Query.GradeRateActivity" />
        <activity android:name=".Student.Query.ExamActivity" />
        <activity android:name=".Student.Query.GradeActivity" />
        <activity android:name=".Student.User.SignatureActivity" />
        <activity android:name=".Student.TimeTable.TimeTableActivity" />
        <activity android:name=".Student.Query.SingleSubjectActivity" />
        <activity android:name=".MainActivity" />
        <activity
            android:name=".Utils.JellyInterpolator"
            tools:ignore="Instantiatable" />
        <activity android:name=".StudentMainActivity"></activity>
        <activity android:name=".WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Student.TimeTable.ui.settime.SetTimeActivity"
            android:theme="@style/Theme.AppCompat" />
        <activity android:name=".Student.TimeTable.ui.login.LoginActivity" />
        <activity
            android:name=".Student.TimeTable.ui.config.ConfigActivity"
            android:theme="@style/Theme.AppCompat" />
        <activity
            android:name=".Student.TimeTable.ui.editcourse.EditActivity"
            android:theme="@style/Theme.AppCompat" />
        <activity android:name=".Student.TimeTable.ui.coursedetails.CourseDetailsActivity" />
    </application>

</manifest>